{
    "name": "ryangjchandler/forge-previewer",
    "description": "Create preview deployments for pull request with Laravel Forge.",
    "keywords": ["forge", "laravel", "preview-deploy"],
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/ryangjchandler/forge-previewer/issues",
        "source": "https://github.com/ryangjchandler/forge-previewer"
    },
    "authors": [
        {
            "name": "Ryan Chandler",
            "email": "support@ryangjchandler.co.uk"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel-zero/framework": "^9.0",
        "laravel/forge-sdk": "^3.13",
        "nunomaduro/termwind": "^1.3",
        "symfony/options-resolver": "^6.3"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.4",
        "pestphp/pest": "^1.21.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["builds/forge-previewer"]
}
